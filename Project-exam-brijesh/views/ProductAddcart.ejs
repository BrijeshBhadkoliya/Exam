    <center>
      <h1>view Products</h1>
   <div class="container">
      <div class="row">
      <% Cratmodel.map((val , i )=>{ %>
          <div class="col-4">
        <div class="card" style="width: 18rem">
          <img src="../<%= val.Image  %>" class="card-img-top" alt="..." style="height: 38  0px;"/>
          <div class="card-body">
            <h3 class="card-title"><%= val.name %></h3>
            <h5 class="card-subtitle"><%= val.price %></h5>
            <h6><%= val.qt %></h6>
            <p class="card-text"><%= val.description %></p>
            <a href="#" class="btn btn-primary">Buy now</a>
            <button>
              <a
                class="dropdown-item"
                href="/Product/deletecart?id=<%= val._id %>"
              >
                <i class="bx bx-edit-alt me-1"></i> Delete Crat
              </a>
            </button>
          </div>
        </div>
      </div>
      <% }) %>
    </div>
  </div>
      <button><a href="/Product/viewProduct">view Products</a></button>
    </center>

